FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1000;

boundaryField
{
    gapBottom
    {
        type            calculated;
        value           uniform 1000;
    }

    gapTop
    {
        type            calculated;
        value           uniform 1000;
    }

    gapOuter
    {
        type            calculated;
        value           uniform 1000;
    }
    gapInner
    {
        type            calculated;
        value           uniform 1000;
    }
}

