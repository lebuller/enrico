FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 4.304);

boundaryField
{
    modInlet
    {
        type           fixedValue;
        value          uniform (0 0 4.304);
    }
    modOutlet
    {
        type            pressureInletOutletVelocity;
//        inletValue      $internalField;
        value           $internalField;
    }
    modInner
    {
        type            noSlip;
    }
    modOuter
    {
        type            zeroGradient;
    }
}

// ************************************************************************* //
