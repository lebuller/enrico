FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    modInlet
    {
        type            calculated;
        value           uniform 0;
    }
    modOutlet
    {
        type            calculated;
        value           uniform 0;
    }
    modInner
    {
        type            compressible::alphatJayatillekeWallFunction;
        Prt             0.85;
        value           uniform 0;
    }
    modOuter
    {
        type            zeroGradient;
        value           uniform 0;
    }
}


